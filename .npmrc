# NPM Configuration for d1-get-started

# Use exact versions when installing dependencies
save-exact=true

# Prevent automatic package-lock.json updates during install
package-lock=true

# Enable long cache for faster installs
# cache can be set to a custom directory if needed
# Example: npm config set cache=/path/to/cache (or npm config set cache /path/to/cache)

# Set the registry (default is https://registry.npmjs.org/)
# Example: npm config set registry=https://registry.npmjs.org/ (or npm config set registry https://registry.npmjs.org/)
